#include<bits/stdc++.h>
using namespace std;

int check[30][30]={0};

string tinhsau(int i) {
	string s;
	s.push_back(i+'0');
	
	return s;
}

int ktra(int x, int y) {
	if((x<=8 && x>=1) && (y<=8 && y>=1)) return 1;
	else return 0;
}

void tinh(int i, int j, int value) {
	for(int k=1; k<=8; k++) {
		if(ktra(i,k)==1) check[i][k] += value;
		if(ktra(k,j)==1) check[k][j] += value;
		
		if(ktra(i+k,j-k)) check[i+k][j-k] += value;
		if(ktra(i-k,j-k)) check[i-k][j-k] += value;
		if(ktra(i-k,j+k)) check[i-k][j+k] += value;
		if(ktra(i+k,j+k)) check[i+k][j+k] += value; 
	}		
}

void lietke(int hang, int cot, string sau) {
	if(cot>8) return;
	if(cot==8) cout<<sau<<endl;
	
	for(int i=1; i<=8; i++) {
		if(check[i][cot+1]==0) {
			tinh(i,cot+1,1);		
			lietke(i, cot+1,sau+ tinhsau(i)+" " +tinhsau(cot+1)+"\n");		
			tinh(i,cot+1,-1);
		}
	}
}

int main() {
	lietke(0,0,"");
}


